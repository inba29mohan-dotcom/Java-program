import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double floatValue = sc.nextDouble();  
        int squareValue = sc.nextInt();       
        int baseValue = sc.nextInt();        
        int powerValue = sc.nextInt();        
        int floorValue = (int)Math.floor(floatValue);
        int ceilValue = (int)Math.ceil(floatValue);
        double sqrtValue = Math.sqrt(squareValue);
        double powValue = Math.pow(baseValue, powerValue);
        System.out.println(floorValue);
        System.out.println(ceilValue);
        System.out.println((int)sqrtValue);
        System.out.println((int)powValue);
    }
}
